{
  "manifest_version": 3,
  "name": "Muslim Diary",
  "version": "0.0.1",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "host_permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": [
      "js/background.js"
    ]
  },
  "action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  }
}
